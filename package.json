{
  "name": "my-lp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --hostname 127.0.0.1 --port 3000",
    "dev:webpack": "next dev --hostname 127.0.0.1 --port 3000",
    "dev:3001": "next dev --turbopack --hostname 127.0.0.1 --port 3001",
    "dev:lan": "next dev --turbopack --hostname 0.0.0.0 --port 3000",
